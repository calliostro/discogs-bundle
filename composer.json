{
    "name": "calliostro/discogs-bundle",
    "description": "Symfony bundle for the Discogs API â€” vinyl, music data & integration made easy",
    "type": "symfony-bundle",
    "keywords": [
        "symfony-bundle",
        "symfony",
        "php",
        "discogs",
        "discogs-api",
        "music",
        "api-client",
        "web-api",
        "audio",
        "vinyl",
        "php8"
    ],
    "homepage": "https://github.com/calliostro/discogs-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Richard van den Brand",
            "email": "richard@vandenbrand.org"
        },
        {
            "name": "calliostro",
            "email": "calliostro@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "calliostro/php-discogs-api": "^2.1",
        "symfony/config": "^6.4|^7.0|^8.0",
        "symfony/dependency-injection": "^6.4|^7.0|^8.0",
        "symfony/http-kernel": "^6.4|^7.0|^8.0",
        "symfony/security-core": "^6.4|^7.0|^8.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^6.4|^7.0|^8.0",
        "phpstan/phpstan": "^2.1",
        "friendsofphp/php-cs-fixer": "^3.87"
    },
    "autoload": {
        "psr-4": {
            "Calliostro\\DiscogsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Calliostro\\DiscogsBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/simple-phpunit",
        "test-coverage": "vendor/bin/simple-phpunit --coverage-html coverage --coverage-clover coverage.xml",
        "cs": "vendor/bin/php-cs-fixer fix --dry-run --diff --verbose",
        "cs-fix": "vendor/bin/php-cs-fixer fix --verbose",
        "analyse": "vendor/bin/phpstan analyse src --level=8",
        "analyse-generics": "vendor/bin/phpstan analyse src --level=8 --configuration .github/phpstan/generic-baseline.neon"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "suggest": {
        "hwi/HWIOAuthBundle": "Enable OAuth support using HWIOAuthBundle"
    }
}
