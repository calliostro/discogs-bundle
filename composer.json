{
    "name": "calliostro/discogs-bundle",
    "description": "Bundle around the Discogs API client for Symfony 6.4 and Symfony 7",
    "type": "symfony-bundle",
    "keywords": ["discogs", "api", "symfony", "bundle"],
    "homepage": "https://github.com/calliostro/CalliostroDiscogsBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Richard van den Brand",
            "email": "richard@vandenbrand.org"
        },
        {
            "name": "calliostro",
            "email": "calliostro@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "calliostro/php-discogs-api": "^2.1.0",
        "symfony/config": "^6.4 || ^7.0",
        "symfony/dependency-injection": "^6.4 || ^7.0",
        "symfony/http-kernel": "^6.4 || ^7.0",
        "symfony/security-bundle": "^6.4 || ^7.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^6.4 || ^7.0"
    },
    "scripts": {
        "test": "vendor/bin/simple-phpunit"
    },
    "autoload": {
        "psr-4": {
            "Calliostro\\DiscogsBundle\\": "src\\"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Calliostro\\DiscogsBundle\\Tests\\": "tests\\"
        }
    },
    "suggest": {
        "hwi/HWIOAuthBundle": "Enable OAuth support using HWIOAuthBundle"
    }
}
